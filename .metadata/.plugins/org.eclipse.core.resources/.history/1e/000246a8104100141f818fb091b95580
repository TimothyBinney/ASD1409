var window1 = Ti.UI.createWindow({
	backgroundColor: "white",
	title: "Friends",
	layout: "vertical",
});

var window2 = Ti.UI.createWindow({
	backgroundColor: "white",
	title: "Add Friend",
	layout: "vertical",
});

var tabs = Ti.UI.createTabGroup();

var tab1 = Ti.UI.createTab({
	icon: "KS_nav_views.png",
	title: "Friends",
	window: window1
});

var tab2 = Ti.UI.createTab({
	icon: "KS_nav_ui.png",
	title: "Add Friend",
	window: window2,
});

tabs.addTab(tab1);
tabs.addTab(tab2);
tabs.open();


win1.add(tabs);
win2.add();

