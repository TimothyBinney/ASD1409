// Main Code

var mainWin = Ti.UI.createWindow({
	backgroundColor: "white",
	fullscreen: true,
});

var mainNav = Ti.UI.createNavigationWindow({
	backgroundColor: "white",
	fullscreen: true,
	window: mainWin,
});

var title = Ti.UI.createView({
	height: 40,
	backgroundColor: "black",
	top: 0,
});

var titleLabel = Ti.UI.createLabel({
	text: "Food Reddit!",
	color: "red",
});

title.add(titleLabel);
win.add(title);
mainNav.open();
