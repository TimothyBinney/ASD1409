var mainWin = Ti.UI.createWindow({
	backgroundColor: "black",
	title: "Pet Store"
});

var mainNav = Ti.UI.iOS.createNavigationWindow({
	window: mainWin
});

var bikeOne = Ti.UI.createView ({
	backgroundColor: "red",
	align: "center",
	top: 5,
	height: 20,
	width: "100%",
});

var bikeOneLabel = Ti.UI.createLabel({
	text: "Log In",
	textAlign: "center",
	color: "#fff",
});


var loadJs = require ("data");


bikeOne.add(bikeOneLabel);
mainWin.add(bikeOne);
mainNav.open();
