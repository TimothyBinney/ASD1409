// Objects in Literal Notation

var bikeOne = {
	"Manufacturer": "Kawasaki",
	"Engine Size": 65,
	"Price": 3699,
	"Bank": 5000,
	"Affordable": function(){
	if(bikeOne.Price < bikeOne.Bank){
			alert("You cannot afford this bike!");
			}
			else
			{
			alert("Congratulations you can afford this bike!");
			}
	
	}



};